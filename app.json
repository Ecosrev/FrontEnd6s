{
  "expo": {
    "name": "Ecosrev",
    "slug": "ecosrev",
    "android": {
      "usesCleartextTraffic": true,
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ],
      "newArchEnabled": true,
      "package": "com.EcosRev.EcosRevApp"
    },
    "web": {},
    "plugins": [
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-font",
      [
        "@react-native-voice/voice",
        {
          "microphonePermission": "Permitir $(PRODUCT_NAME) acessar o microfone para reconhecimento de voz.",
          "speechRecognitionPermission": "Permitir $(PRODUCT_NAME) usar reconhecimento de fala."
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "a5c946f2-fbd0-422a-958f-8b11a7523b36"
      }
    }
  }
}
