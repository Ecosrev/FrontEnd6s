{
  "expo": {
    "name": "EcosRev",
    "slug": "ecosrev",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "plugins": [
      [
        "expo-build-properties",
        {
          "android": {
            "useAndroidX": true,
            "enableJetifier": true
          }
        }
      ],
      [
        "expo-speech-recognition",
        {
          "microphonePermission": "Allow EcosRev to access your microphone",
          "speechRecognitionPermission": "Allow EcosRev to use speech recognition"
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "14753a07-e548-40ce-8cc0-8daecb802e26"
      }
    },
    "android": {
      "permissions": [
        "android.permission.RECORD_AUDIO"
      ],
      "package": "com.jaolucas.ecosrev"
    }
  }
}
